#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jacobmckenney/code/projects/abide/node_modules/.pnpm/json-diff@0.9.0/node_modules/json-diff/bin/node_modules:/Users/jacobmckenney/code/projects/abide/node_modules/.pnpm/json-diff@0.9.0/node_modules/json-diff/node_modules:/Users/jacobmckenney/code/projects/abide/node_modules/.pnpm/json-diff@0.9.0/node_modules:/Users/jacobmckenney/code/projects/abide/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jacobmckenney/code/projects/abide/node_modules/.pnpm/json-diff@0.9.0/node_modules/json-diff/bin/node_modules:/Users/jacobmckenney/code/projects/abide/node_modules/.pnpm/json-diff@0.9.0/node_modules/json-diff/node_modules:/Users/jacobmckenney/code/projects/abide/node_modules/.pnpm/json-diff@0.9.0/node_modules:/Users/jacobmckenney/code/projects/abide/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../json-diff@0.9.0/node_modules/json-diff/bin/json-diff.js" "$@"
else
  exec node  "$basedir/../../../../../json-diff@0.9.0/node_modules/json-diff/bin/json-diff.js" "$@"
fi
